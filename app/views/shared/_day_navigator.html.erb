<% if logged_in? %>
<div class="day-navigator">
    <a href="<%= base_url + "/" + previous_day %>"><i class="fa fa-chevron-circle-left"></i></a>
    <span class="current-day"><%= nice_day(current_day) %></span>
    <a href="<%= base_url + "/" + next_day %>"><i class="fa fa-chevron-circle-right"></i></a>
</div>
<% end %>
